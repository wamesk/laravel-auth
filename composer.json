{
    "name": "wamesk/laravel-auth",
    "version": "4.0.0",
    "description": "OAuth2 authorization with API endpoints. Also includes registration process, login, password reset, email validation.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "WAME s.r.o.",
            "email": "info@wame.sk"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Wame\\LaravelAuth\\": "src",
            "Wame\\LaravelAuth\\Database\\Factories\\": "database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wame\\LaravelAuth\\Providers\\LaravelAuthServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.0",
        "laravel/framework": "^10.0|^11.0",
        "hisorange/browser-detect": "^5.0",
        "wamesk/core": "dev-master"
    }
