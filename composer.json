{
    "name": "wamesk/laravel-auth",
    "description": "OAuth2 authorization with API endpoints. Also includes registration process, login, password reset, email validation.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "WAME s.r.o.",
            "email": "info@wame.sk"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Wame\\LaravelAuth\\": "src"
        }
